    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_",
    "include": [
      "./README.md",
      "./ChangeLog.md",
      "./src/entry.js"
    ],
    "exclude": [
    ]
